<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CoinAnimation">
        <attr name="circleCount" format="integer" />
        <attr name="xAxisDir">
            <flag name="none" value="0" />
            <flag name="clockwise" value="1" />
            <flag name="abtucclockwise" value="-1" />
        </attr>
        <attr name="yAxisDir">
            <flag name="none" value="0" />
            <flag name="clockwise" value="1" />
            <flag name="abtucclockwise" value="-1" />
        </attr>
        <attr name="zAxisDir">
            <flag name="none" value="0" />
            <flag name="clockwise" value="1" />
            <flag name="abtucclockwise" value="-1" />
        </attr>
        <attr name="result">
            <flag name="front" value="1" />
            <flag name="reverse" value="-1" />
        </attr>
        <attr name="headDrawable" format="reference"/>
        <attr name="tailDrawable" format="reference"/>
        <attr name="durationTime" format="integer"/>
        <attr name="startOffset" format="integer"/>
    </declare-styleable>
</resources>